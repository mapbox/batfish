{
  "extends": "./eslint-base.json",
  "env": {
    "browser": false,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "script"
  },
  "plugins": [
    "node"
  ],
  "rules": {
    "node/no-unsupported-features": ["error", {"version": 6}],
    "node/no-missing-require": ["error"]
  }
}

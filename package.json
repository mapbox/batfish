{
  "name": "@mapbox/batfish",
  "private": true,
  "version": "0.0.0",
  "description": "The React-powered static-site generator you didn't know you wanted",
  "main": "index.js",
  "bin": {
    "batfish": "bin/batfish.js"
  },
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --single-quote --write '{lib,src,bin,test}/**/*.js'",
    "test-jest": "jest",
    "pretest": "npm run lint",
    "test": "echo \"No tests yet\"",
    "demo": "bin/batfish.js -c demo/batfish.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapbox/batfish.git"
  },
  "author": "Mapbox",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mapbox/batfish/issues"
  },
  "homepage": "https://github.com/mapbox/batfish#readme",
  "lint-staged": {
    "{lib,src,bin,test}/**/*.js": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "jest": {
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ],
    "resetMocks": true,
    "roots": [
      "./test"
    ],
    "testRegex": ".*\\.jest\\.js$"
  },
  "dependencies": {
    "@mapbox/babel-plugin-transform-md-react": "^0.1.0",
    "@mapbox/css-sieve": "^0.1.1",
    "@mapbox/md-react-transformer-loader": "^0.2.0",
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx-self": "^6.22.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-register": "^6.24.1",
    "browser-sync": "^2.18.12",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "cp-file": "^4.2.0",
    "css-loader": "^0.28.4",
    "csso": "^3.1.1",
    "date-time": "^2.0.0",
    "debounce": "^1.0.2",
    "del": "^3.0.0",
    "es6-promise": "^4.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "get-port": "^3.1.0",
    "globby": "^6.1.0",
    "got": "^7.0.0",
    "gray-matter": "^2.1.1",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "meow": "^3.7.0",
    "mkdirp": "^0.5.1",
    "optimize-js": "^1.0.3",
    "pify": "^3.0.0",
    "postcss": "^6.0.1",
    "postcss-loader": "^2.0.5",
    "postcss-url": "^7.0.0",
    "postcss-values-parser": "^1.2.2",
    "prop-types": "^15.5.10",
    "style-loader": "^0.18.2",
    "uglify-js": "^3.0.15",
    "webpack": "^2.6.1",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0",
    "xtend": "^4.0.1"
  },
  "peerDependencies": {
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "react-helmet": "^5.1.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^4.0.0",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-prettier": "^2.1.1",
    "eslint-plugin-react": "^7.0.1",
    "husky": "^0.13.4",
    "jest": "^20.0.4",
    "lint-staged": "^3.6.0",
    "prettier": "^1.4.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.1.3"
  },
  "optionalDependencies": {}
}
